{
    "appUrl": "/__dirname/<%= settings.appDir %>/",
    "appModule": "c6.embed",
    "distDir": "dist",
    "appDir": "app",
    "awsJSON": ".aws.json",
    "saucelabsJSON": ".saucelabs.json",
    "browserstackJSON": ".browserstack.json",
    "sandboxPort": 9000,
    "collateralDir": "c6Content",
    "experiencesJSON": "<%= settings.appDir %>/assets/mock/experiences.json",
    "libUrl": "http://s3.amazonaws.com/c6.dev/ext/",
    "defaultE2EEnv": "browserstack",
    "s3": {
        "test": {
            "bucket": "c6.dev",
            "app": "ext/<%= package.name %>/v1/"
        },
        "testStandalone": {
            "bucket": "com.cinema6.staging.glickm",
            "app": "solo"
        },
        "production": {
            "bucket": "c6.ext",
            "app": "<%= package.name %>/v1/"
        },
        "productionStandalone": {
            "bucket": "com.cinema6",
            "app": "solo"
        }
    }
}
